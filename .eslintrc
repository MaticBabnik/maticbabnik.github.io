{
    "extends": ["@nuxtjs/eslint-config-typescript", "prettier"],
    "rules": {
        "indent": ["warn", 4],
        "vue/html-indent": ["error", 4],
        "semi": ["warn", "always"],
        "quotes": ["warn", "single", { "avoidEscape": true }],
        "space-before-function-paren": ["warn", "never"],
    },
}
